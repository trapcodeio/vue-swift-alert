{
  "name": "vue-swift-alert",
  "version": "2.0.0",
  "description": "Simple Hide and Show Vue Package.",
  "main": "dist/swift-alert.umd.js",
  "module": "dist/swift-alert.esm.js",
  "unpkg": "dist/swift-alert.min.js",
  "browser": {
    "./sfc": "src/my-component.vue"
  },
  "types": "index.d.ts",
  "author": "trapcodeio",
  "repository": "https://github.com/trapcodeio/vue-swift-alert",
  "scripts": {
    "watch": "npx tsc --watch",
    "prepublishOnly": "npx tsc",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "npx rollup --config ./rollup.config.js --format umd --file dist/swift-alert.umd.js",
    "build:es": "npx rollup --config ./rollup.config.js --format es --file dist/swift-alert.esm.js",
    "build:unpkg": "npx rollup --config ./rollup.config.js --format iife --file dist/swift-alert.min.js"
  },
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^21.0.0",
    "rollup": "^2.58.0",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "^4.4.3",
    "vue": "^3.2.11",
    "vue-template-compiler": "^2.6.14"
  }
}
